# ===== Google credentials & additional info =====
GOOGLE_APPLICATION_CREDENTIALS=/path/to/credentials.json
GOOGLE_DOC_ID=your-google-docs-id
DOC_SYNC_MODE=rebuild | append
DOC_SECTION='Java programming language > Bài tập > Bài tập trên codeptit'
ENABLE_DOCS=true         # set to true to actually write to Docs
DRY_RUN=false            # preview only; set to false to perform writes

# ===== Target submissions list URL =====
LIST_URL=https://code.ptit.edu.vn/student/history


# ===== Auth via browser cookies =====
# AUTO_LOGIN: set to true to use Selenium to auto-login and fetch cookies
AUTO_LOGIN=true
LOGIN_URL=https://code.ptit.edu.vn/login
LOGIN_USERNAME=B23DCCC123
LOGIN_PASSWORD=your-password
USERNAME_SELECTOR=#login__user
PASSWORD_SELECTOR=#login__pw
SUBMIT_SELECTOR=button[type='submit']
# COOKIE_STRING: fallback manual cookie string (copied from browser)
COOKIE_STRING=
# Optional: custom user agent to look like a normal browser
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120 Safari/537.36
MAX_PAGE=1
# ===== Scraper selectors =====
# If the table is a standard <table>, these defaults often work.
ROW_SELECTOR=table tr
COL_INDEXES=0,1,2,3,4,5,6